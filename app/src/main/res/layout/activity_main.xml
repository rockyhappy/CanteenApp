<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".SplashScreen">

    <View
        android:id="@+id/vector_1"
        android:layout_width="match_parent"
        android:layout_height="168.39dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/upper"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.32999998"
        app:srcCompat="@drawable/ion_home"
        android:contentDescription="TODO" />

    <View
        android:id="@+id/vector_2"
        android:layout_width="match_parent"
        android:layout_height="168.39dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/splashscreen_lower_border"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <com.caverock.androidsvg.SVGImageView
        android:id="@+id/svgImageView"
        android:layout_width="210dp"
        android:layout_height="63dp"
        android:layout_marginTop="165dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView2" />

    <ImageView
        android:id="@+id/brunchBliss"
        android:layout_width="139dp"
        android:layout_height="19dp"
        android:layout_marginTop="53dp"
        app:layout_constraintEnd_toEndOf="@+id/svgImageView"
        app:layout_constraintStart_toStartOf="@+id/svgImageView"
        app:layout_constraintTop_toTopOf="@+id/svgImageView"
        app:srcCompat="@drawable/_crave_click_enjoy_"
        android:contentDescription="TODO" />


</androidx.constraintlayout.widget.ConstraintLayout>